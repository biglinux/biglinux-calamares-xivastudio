pkgname=biglinux-calamares-xivastudio
pkgver=$(date +%y.%m.%d)
pkgrel=$(date +%H%M)
pkgdesc="Calamares configs for BigLinux Community XIVA Studio"
arch=('any')
url="https://github.com/biglinux/$pkgname"
license=('GPL3')
# depends=('')
provides=("$pkgname")
# conflicts=('')
source=("git+${url}.git")
sha256sums=('SKIP')

package() {
    cp -r "${srcdir}/${pkgname}/usr/" "${pkgdir}/"
}
